<script type="text/javascript">
$(document).ready(function () {
$("#owl-demo").owlCarousel({
autoPlay: 3000,
items: 1,
rewindNav: true,
itemsDesktop: [1199, 1],
itemsDesktopSmall: [979, 1],
callbacks: false
});
});  
