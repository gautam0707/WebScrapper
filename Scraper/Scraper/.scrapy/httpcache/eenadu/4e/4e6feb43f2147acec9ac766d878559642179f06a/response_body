    var crtg_split = (crtg_content || '').split(';');
    var cto_values = {};
    for (var i=1;i<crtg_split.length;i++){
        var key = (crtg_split[i-1].split('='))[0];
        var value = (crtg_split[i-1].split('='))[1];
        cto_values[key] = cto_values[key] || [];
        cto_values[key].push( "" + value + "");
    }
    for (var i in cto_values) {
        if (cto_values.hasOwnProperty(i)) googletag.pubads().setTargeting("" + i + "", "" + cto_values[i] + "");
    }
});
</script>

